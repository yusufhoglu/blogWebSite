<%- include("header"); -%>

<main>
    <ul class="homepage-list">
        <% for(var i = post.length-1; i >= 0; i--){ %>
            <li class="header">
                <a href="/posts/<%=post[i]._id%>" class="page-divisor">
                <ul >
                    <li class="item"><img src="<%= post[i].ImageUrl %>"></li>
                    <li class="item"><p><%= post[i].Title %><p/></li>
                </ul>   
                </a>
            </li>
        <% } %>
    </ul>
</main>

<%- include("footer"); -%>
